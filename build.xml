<project name="Insulin" default="compile" basedir="." xmlns:ivy="antlib:org.apache.ivy.ant">	<get src="https://raw.github.com/Trylogic/AntHelpers/1.0/build.ivy.xml" dest="." usetimestamp="true" ignoreerrors="true" />	<import file="build.ivy.xml" />	<get src="https://raw.github.com/Trylogic/AntHelpers/1.0.1/build.flexsdk.xml" dest="." usetimestamp="true" ignoreerrors="true" />	<import file="build.flexsdk.xml" />	<get src="https://raw.github.com/Trylogic/AntHelpers/1.0/ivysettings.github.xml" dest="." usetimestamp="true" ignoreerrors="true" />	<get src="https://raw.github.com/Trylogic/AntHelpers/1.0/ivysettings.publish.local.xml" dest="." usetimestamp="true" ignoreerrors="true" />	<property name="out" value="${basedir}/out" />	<property name="asdocOutput" value="${basedir}/asdoc" />	<property name="flex.download.branch" value="4.6" />	<property name="flex.download.version" value="flex_sdk_4.6.0.23201_mpl" />	<target name="publish" depends="compile" description="Publish">		<ivy:publish resolver="local" forcedeliver="true">			<artifacts pattern="${basedir}/out/[artifact].[ext]" />		</ivy:publish>	</target>	<target name="compile" description="compile the source" depends="IvyHelper.update">		<downloadFlexSDK branch="${flex.download.branch}" version="${flex.download.version}" />		<mkdir dir="${out}" />		<compc output="${out}/${ant.project.name}.swc" dir="${basedir}" fork="true">			<load-config filename="${basedir}/compiler-config.xml" append="true" />		</compc>	</target>	<target name="asdoc">		<downloadFlexSDK branch="${flex.download.branch}" version="${flex.download.version}" />		<mkdir dir="${asdocOutput}" />		<asdoc output="${asdocOutput}">			<doc-sources path-element="${basedir}/src/" />		</asdoc>	</target>	<target name="clean" description="clean up">		<delete dir="${out}" failonerror="no" />		<delete dir="asdoc" failonerror="no" />	</target>	<target name="tag">		<xmlproperty file="ivy.xml" collapseAttributes="true"/>		<exec executable="git" failonerror="true" failifexecutionfails="true">			<arg value="tag" />			<arg value="-a" />			<arg value="v${ivy-module.info.revision}" />			<arg value="-m 'v${ivy-module.info.revision}'" />		</exec>	</target></project>